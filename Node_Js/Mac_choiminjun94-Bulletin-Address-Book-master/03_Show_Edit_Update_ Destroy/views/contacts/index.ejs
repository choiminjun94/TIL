<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/head') %>
    <!-- include %>를 사용해서 외부 ejs(head)를 가져왔습니다. --> 
  </head>
  <body>
    <%- include('../partials/nav') %>
    <!-- include %>를 사용해서 외부 ejs(nav)를 가져왔습니다. -->

    <div class="contact contact-index">
      <h2>Index</h2>
      <ul>
        <% contacts.forEach(function(contact) { %>
          <!-- contacts object에 forEach를 사용해서 코드를 반복해서 표시하고있습니다 -->
          <li>
            <a href="/contacts/<%= contact._id %>"><%= contact.name %></a> 
          </li>
        <% }) %>
      </ul>
    </div>
  </body>
</html>